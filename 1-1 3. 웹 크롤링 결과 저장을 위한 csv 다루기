# csv 모듈
import usecsv, os
os.chdir(r'C:\DEV\workspace03\ws01_python-test')
ad=[[1,2,3],['우리','나라','만세'],['나는','우리나라가','좋더라']]
usecsv.writecsv('new_write_file.csv', ad)

total = usecsv.opencsv('new_write_file.csv')
print(total)
